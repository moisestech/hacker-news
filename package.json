{
  "name": "hacker-news",
  "version": "1.0.0",
  "description": "Hacker News clone build with React, Hooks, ES8, RouterV6",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "NODE_ENV === 'production' webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moisestech/hacker-news.git"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "syntax-dynamic-import"
    ]
  },
  "keywords": [
    "ReactJS",
    "Hacker News",
    "Reach Hooks"
  ],
  "author": "Moises Sanabria",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/moisestech/hacker-news/issues"
  },
  "homepage": "https://github.com/moisestech/hacker-news#readme",
  "dependencies": {
    "prop-types": "15.7.2",
    "query-string": "^6.13.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "babel-loader": "8.1.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "copy-webpack-plugin": "6.1.0",
    "css-loader": "4.2.2",
    "html-webpack-plugin": "4.4.1",
    "style-loader": "1.2.1",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  }
}
